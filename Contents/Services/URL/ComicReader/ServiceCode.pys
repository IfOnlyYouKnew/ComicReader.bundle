import formats
from __builtin__ import dir, getattr


def MetadataObjectForURL(url):
    return PhotoObject(title="na", summary="na")


def MediaObjectsForURL(url):
    return [MediaObject(parts=[PartObject(key=Callback(GetImage, url=url))])]


def GetImage(url):
    archive, filename, fmt, user = url.split('comicreader://')[-1].split('|')
    return formats.get_image(archive, filename, fmt, user)
